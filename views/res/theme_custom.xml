<?xml version="1.0" encoding="utf-8"?>
<theme 
    id="theme_custom" 
    parent="theme_default"
    >
    <color id="window_background" value="#dedede"/>
    <style id="BUTTON"
        align="Center"
        margins="2pt,2pt,2pt,2pt"
        padding="1pt,1pt,1pt,1pt"
        fontSize="100%"
        fontWeight="bold"
        backgroundColor="#e4ede9"
        >
        <state state_enabled="false" textColor="#C0000000"/>
        <state state_pressed="true" backgroundColor="#8080FF"/>
        <state state_focused="true" state_hovered="true" backgroundColor="#5c5c5c" textColor="#FF8000"/>
        <state state_focused="true" backgroundColor="#C0E0FF"/>
        <state state_focused="false" state_hovered="true" textColor="#FF8000"/>
    </style>
</theme>
